{
  "name": "sinmido",
  "version": "1.0.0",
  "private": true,
  "description": "sinmido",
  "author": "seagod333",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "build:wp": "gatsby build --prefix-paths",
    "build:clean": "powershell -Command \"Remove-Item .cache -Recurse -Force -ErrorAction SilentlyContinue; Remove-Item public -Recurse -Force -ErrorAction SilentlyContinue; npm run build\"",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "css:build": "postcss ./src/assets/styles/tailwind.css --config ./postcss.config.js -o ./src/assets/styles/output.css",
    "css:watch": "postcss ./src/assets/styles/tailwind.css --config ./postcss.config.js -o ./src/assets/styles/output.css --watch"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "framer-motion": "^12.23.24",
    "gatsby": "^5.14.6",
    "gatsby-plugin-image": "^3.15.0",
    "gatsby-plugin-output": "^0.1.3",
    "gatsby-plugin-postcss": "^6.15.0",
    "gatsby-plugin-sass": "^6.15.0",
    "gatsby-plugin-sharp": "^5.15.0",
    "gatsby-source-wordpress": "^7.16.0",
    "gatsby-transformer-sharp": "^5.15.0",
    "react": "^18.2.0",
    "react-countup": "^6.5.3",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "sass": "^1.69.5",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.3.3"
  }
}
